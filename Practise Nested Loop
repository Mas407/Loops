#include<iostream>
using namespace std;
int main(){
    for(int i=1; i<=5; i++){
        for(int j=1; j<=i; j++){
            cout<<"*";
        }cout<<endl;
    }
int n=1;
    for(int i=1; i<=4; i++){
    for(int j=1; j<=4; j++){
        cout<<j;
    }cout<<endl;
}



for(int i=1; i<=4; i++){
    for(int k=1; k<=4-i; k++)
    cout<<" ";
    for(int j=1; j<=i; j++)
        cout<<"* ";
    cout<<endl;
}




for(int i=1; i<=5; i++){
    for(int j=1; j<=5-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++ )
    cout<<"& ";
    cout<<endl;
}




for(int i=1; i<=4; i++){
    for(int j=1; j<=4-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++){
        cout<<k<<" ";
    }cout<<endl;
}






for(int i=5; i>=1; i--){
    for(int j=1; j<=5-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<k<<" ";
    cout<<endl;
}


for(int i=1; i<=4; i++){
    for(int j=1; j<=4-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<"* ";
    cout<<endl;
}
for(int i=4; i>=1; i--){
    for(int j=1; j<=4-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<"* ";
    cout<<endl;
}




for(int i=5; i>=1; i--){
     for(int j=1; j<=5-i; j++)
     cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<"*"<<" ";
    cout<<endl;
}

for(int i=2; i<=5; i++){
    for(int j=1; j<=5-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<"*"<<" ";
    cout<<endl;
}

for(int i=1; i<=5; i++){
    for(int j=1; j<=5-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<k<<" ";
    cout<<endl;
}

for(int i=4; i>=1; i--){
    for(int j=1; j<=5-i; j++)
    cout<<" ";
    for(int k=1; k<=i; k++)
    cout<<k<<" ";
    cout<<endl;
}


return 0;
}
