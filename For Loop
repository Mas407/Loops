Program 2

int p=1;
for(int i=1; i<=10; i++){
    if(i%2==1){
        p*=i;
    }
}
cout<<"Result is = "<<p;


Program 3

int a,b;
cout<<"Enter the table numbrt ::> ";
cin>>a;
cout<<"Enter the lenght of the table ";
cin>>b;
for(int i=1; i<=b; i++){
    cout<<a<<" * "<<i<<" = "<<a*i<<endl;
}


Program 4

int n,sum=0;
cout<<"Enter the number ";
cin>>n;
for(int i=1; i<=n; i++){
    sum+=pow(i,2);
}
cout<<sum;


Program 5


float c;
cout<<"Fahrenheit "<<setw(10)<<"Celsius"<<endl;
for(int i=50; i<=100; i=i+5){
    c=5/9.0*(i-32);
    cout<<i<<setw(10)<<c<<endl;
    
}


Program 6


int n;
int digit;
cout<<"Enter a number ";
cin>>n;
int max=-1; int min=10;
for( ; n!=0 ; ){
    digit=n%10;
 if(digit>max)
 max=digit;
 if(digit<min)
 min=digit;
 n=n/10;
}
cout<<"Heigest digit is = "<<max<<endl<<"Mindigit is = "<<min;



    return 0;
}
